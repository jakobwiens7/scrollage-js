  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.2",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}