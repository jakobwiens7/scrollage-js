{
  "name": "scrollagejs",
  "version": "1.0.0",
  "description": "A lightweight JavaScript library that animates elements based on the scroll position.",
  "author": "Jakob Wiens",
  "license": "MIT",
  "keywords": [
    "scrolling",
    "animation",
    "parallax"
  ],
  "main": "dist/scrollage.min.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
